    "style-loader": "3.3.1",
    "stylelint": "14.10.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-middleware": "5.3.3",
