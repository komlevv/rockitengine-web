{
  "name": "rockitengine-web",
  "version": "1.0.0",
  "description": "Rockitengine website",
  "scripts": {
    "express-dev": "NODE_ENV=development nodemon --config nodemon.dev.json -L src/server/serverDev.js",
    "express-prod": "NODE_ENV=production node dist/server.js",
    "build-all": "NODE_ENV=production node -r @babel/register tools/build.js",
